{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:mattstauffer/joindin-client.git"
        },
        {
            "type": "git",
            "url": "git@github.com:mattstauffer/laravel-gravatar.git"
        }
    ],
    "type": "project",
    "require": {
        "laravel/framework": "5.3.*",
        "rexxars/joindin-client": "^0.0.1",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "laravelcollective/html": "5.3.*",
        "doctrine/dbal": "~2.5",
        "thomaswelton/laravel-gravatar": "dev-laravel5-support",
        "thujohn/twitter": "dev-master",
        "maknz/slack": "~1.5",
        "lucadegasperi/oauth2-server-laravel": "5.2.*",
        "recaptcha/php5": "^2.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "intervention/image": "^2.3",
        "php": ">=5.6.4",
        "pragmarx/firewall": "^1.0",
        "laravel/scout": "^2.0",
        "algolia/algoliasearch-client-php": "^1.14"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "laracasts/testdummy": "~2.3",
        "mockery/mockery": "0.9.*",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*",
        "filp/whoops": "^2.1",
        "tightenco/mailthief": "^0.3.2"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Http/Controllers/"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/IntegrationTestCase.php",
            "tests/Api/ApiTestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    }
}
